{
  "name": "survivejs-site",
  "private": true,
  "version": "2.0.0",
  "description": "SurviveJS site",
  "author": "Juho Vepsalainen <bebraw@gmail.com>",
  "scripts": {
    "start": "node ./antwar.bootstrap.js develop",
    "build": "npm run fetch:books && node ./antwar.bootstrap.js build && npm run sitemap && npm run copy:all",
    "deploy": "gh-pages -d build && npm run purge_cf",
    "fetch:books": "scripts/fetch-books.sh",
    "bootstrap": "scripts/bootstrap.sh",
    "sitemap": "cd build && sitemap-static --prefix=https://survivejs.com/ > sitemap.xml",
    "lint": "eslint . --ext js --ext jsx --ignore-path .gitignore --cache",
    "purge_cf": "node ./utils/purge_cf.js",
    "test": "npm run lint",
    "check:links": "hyperlink build/index.html -r",
    "prepush": "npm test",
    "copy:all": "npm run copy:extra",
    "copy:demos": "cp -rf ../react/project_source/builds/ build/demos/",
    "copy:extra": "cp -rf assets/extra/ build/"
  },
  "dependencies": {
    "@survivejs/components": "^0.2.0",
    "emotion": "^8.0.12",
    "front-matter": "^2.3.0",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.7",
    "moment": "^2.19.4",
    "prism-languages": "^0.4.0",
    "prismjs": "github:bebraw/prism#backend-hooks",
    "react": "^16.2.0",
    "react-emotion": "^8.0.12",
    "react-router-dom": "^4.2.2",
    "remove-markdown": "^0.2.2",
    "tamia-theme": "^0.1.0"
  },
  "devDependencies": {
    "antwar": "^0.21.1",
    "antwar-rss-plugin": "^0.21.0",
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-emotion": "^8.0.12",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "^0.1.17",
    "cloudflare4": "^0.2.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.3.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "hyperlink": "^3.0.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.9.2",
    "raw-loader": "^0.5.1",
    "react-router": "^4.2.0",
    "sass-loader": "^6.0.6",
    "serve-static": "^1.13.1",
    "sitemap-static": "^0.4.2",
    "style-loader": "^0.19.0",
    "title-case": "^2.1.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/survivejs/site.git"
  },
  "homepage": "https://www.survivejs.com",
  "bugs": {
    "url": "https://github.com/survivejs/site/issues"
  },
  "keywords": [
    "site",
    "antwar"
  ],
  "licenses": "MIT"
}
